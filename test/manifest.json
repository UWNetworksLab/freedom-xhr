{
    "manifest_version": 2,

    "app": {
        "background": {
            "scripts": ["main.js"]
        }
    },

    "version": "0.0.0",
    "name": "chrome.sockets.tcp.xhr",
    "author": "Ahmad Nassri <ahmad@codeinchaos.com> (http://ahmadnassri.com)",

    "offline_enabled": true,

    "sockets": {
        "tcp": {
            "connect": "*:*"
        }
    },

    "permissions": [
        {
            "socket": ["tcp-connect:*:*"]
        }
    ]
}
